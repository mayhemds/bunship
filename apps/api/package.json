{
  "name": "@bunship/api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run --env-file=../../.env --watch src/index.ts",
    "dev:worker": "bun run --env-file=../../.env --watch src/worker.ts",
    "start": "bun run --env-file=../../.env src/index.ts",
    "start:worker": "bun run --env-file=../../.env src/worker.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.515.0",
    "@aws-sdk/s3-request-presigner": "^3.515.0",
    "@bunship/config": "workspace:*",
    "@bunship/database": "workspace:*",
    "@bunship/emails": "workspace:*",
    "@bunship/utils": "workspace:*",
    "@elysiajs/cors": "^1.0.0",
    "@elysiajs/swagger": "^1.0.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sinclair/typebox": "^0.34.48",
    "bullmq": "^5.4.0",
    "drizzle-orm": "^0.45.1",
    "elysia": "^1.0.0",
    "elysia-helmet": "^2.0.0",
    "elysia-rate-limit": "^4.0.0",
    "ioredis": "^5.3.2",
    "jose": "^5.2.0",
    "otpauth": "^9.4.1",
    "resend": "^3.2.0",
    "stripe": "^14.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.4.0"
  }
}
